{
    "compilerOptions": {
        "target": "ESNext",
        "module": "ESNext",
        "jsx": "react-jsx",
        "declaration": true,
        "esModuleInterop": true,
        "skipLibCheck": true,
        "strict": true,
        "moduleResolution": "Node",
        "outDir": "dist",
        "plugins": [
            {
                "name": "@effect/language-service"
            }
        ]
    },
    "include": ["src"]
}
